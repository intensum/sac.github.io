{
	"id": "com.fd.gaugeled",
	"version": "1.0.0",
	"name": "Gauge LED @ Great Intensum",
	"description": "A Gauge LED demo @ Intensum",
	"newInstancePrefix": "Gauge",
	"icon": "http://localhost/SAC/sacgaugeled/box/icon.png",
	"vendor": "INTENSUM Luxembourg",
	"eula": "EULA",
	"license": "2.0",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-fd-gaugeled",
			"url": "http://localhost/SAC/sacgaugeled/box/box.js",
			"integrity": "",
			"ignoreIntegrity": true
		},{
			"kind": "builder",
			"tag": "com-fd-gaugeled-bps",
			"url": "http://localhost/SAC/sacgaugeled/box/box_bps.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"value": {
			"type": "string",
			"description": "Gauge value",
			"default": ""
		},
		"title": {
			"type": "string",
			"description": "Gauge title",
			"default": ""
		},
		"subtitle": {
			"type": "string",
			"description": "Gauge subtitle",
			"default": ""
		},
		"min": {
			"type": "integer",
			"description": "Gauge min",
			"default": 0
		},
		"max": {
			"type": "integer",
			"description": "Gauge max",
			"default": 10
		},
		"width": {
			"type": "integer",
			"default": 50
		},
		"height": {
			"type": "integer",
			"default": 50
		}
	},
	"methods": {
		"setValue": {
			"description": "Sets the Gauge value.",
			"parameters": [
				{
					"name": "newValue",
					"type": "string",
					"description": "Gauge Value"
				}
			],
			"body": "this.value = newValue;"
		},
		"getValue": {
			"returnType": "string",
			"description": "Returns the Gauge value.",
			"body": "return this.value;"
		}
	},
	"events": {
		"onClick": {
			"description": "Called when the user clicks the Box."
		}
	}
